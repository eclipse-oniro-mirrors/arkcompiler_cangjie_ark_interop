import kit.PerformanceAnalysisKit.Hilog
@Test
class testGetTime_1 {
    /*
     * @tc.number    : Sub_Cangjie_testGetTime_getTime_0100
     * @tc.name      : TestCase_9
     * @tc.desc      : get system time.
     * @tc.size      : MediumTest
     * @tc.type      : Function
     * @tc.level     : Level 0
     */
    @TestCase
    @Tag[APILevel12, TestLevel0]
    func TestCase_9(): Unit {
        Hilog.info(0, "cangjietest", "testcase TestCase_9 begins.")
        Hilog.info(0, "cangjietest", "define local variable time")
        let time = SystemDateTime.getTime(isNanoseconds: true)
        Hilog.info(0, "cangjietest", "define local variable timeString")
        let timeString = time.toString()
        Hilog.info(0, "cangjietest", "define local variable r")
        let r = Regex("\\d+")
        Hilog.info(0, "cangjietest", "define local variable res")
        let res = r.matches(timeString)
        @Assert(res, true)
        Hilog.info(0, "cangjietest", "testcase TestCase_9 ends.")
    }

