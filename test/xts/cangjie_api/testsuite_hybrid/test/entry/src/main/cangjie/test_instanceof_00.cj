package ohos_app_cangjie_entry

import ohos.ark_interop.{JSModule, JSContext, JSCallInfo, JSValue, JSClass, JSObject, SharedObject, 
    JSRuntime}

let _ = JSModule.registerFunc("testInstanceOf00", testInstanceOf00)

func testInstanceOf00(jsContext: JSContext, _: JSCallInfo): JSValue {
    let jsClass = jsContext.clazz(
        {
            jsContext: JSContext, jsCallInfo: JSCallInfo =>
            let jsObject = jsContext.object()
            jsObject.toJSValue()
        }
    )
    let jsObject = jsClass.new().asObject()
    Assert.isTrue(
        jsObject.instanceOf(jsClass),
        reason: "jsObject created by calling jsClass.new() is an instance of jsClass"
    )
    jsContext.undefined().toJSValue()
}
